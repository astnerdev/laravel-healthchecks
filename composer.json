{
    "name": "renoki-co/laravel-healthchecks",
    "description": "Laravel Healthchecks is a simple controller class that helps you build your own healthchecks endpoint without issues.",
    "keywords": ["laravel", "php", "healthcheck", "controller", "server", "checks", "endpoint"],
    "license": "MIT",
    "homepage": "https://github.com/renoki-co/laravel-healthchecks",
    "authors": [
        {
            "name": "Alex Renoki",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/framework": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "RenokiCo\\LaravelHealthchecks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenokiCo\\LaravelHealthchecks\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "orchestra/database": "^4.0|^5.0|^6.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {	
        "laravel": {	
            "providers": [	
                "RenokiCo\\LaravelHealthchecks\\LaravelHealthcheckServiceProvider"	
            ]	
        }	
    }
}
