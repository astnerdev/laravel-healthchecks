{
    "name": "renoki-co/laravel-healthchecks",
    "description": "Laravel Healthchecks is a simple controller class that enables the possibility of building your own healthchecks endpoint without issues.",
    "keywords": ["laravel", "php", "healthcheck", "controller", "server", "checks", "endpoint"],
    "license": "MIT",
    "homepage": "https://github.com/renoki-co/laravel-healthchecks",
    "authors": [
        {
            "name": "Alex Renoki",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "laravel/framework": "^6.2|^7.0"
    },
    "autoload": {
        "psr-4": {
            "RenokiCo\\LaravelHealthchecks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenokiCo\\LaravelHealthchecks\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "orchestra/database": "^5.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "RenokiCo\\LaravelHealthchecks\\LaravelHealthcheckserviceProvider"
            ]
        }
    }
}
