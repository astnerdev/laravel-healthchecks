{
    "name": "renoki-co/laravel-healthchecks",
    "description": "Laravel Healthchecks is a simple controller class that helps you build your own healthchecks endpoint without issues.",
    "keywords": [
        "laravel",
        "php",
        "healthcheck",
        "controller",
        "server",
        "checks",
        "endpoint"
    ],
    "license": "Apache-2.0",
    "homepage": "https://github.com/renoki-co/laravel-healthchecks",
    "authors": [
        {
            "name": "Alex Renoki",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/routing": "^10",
        "illuminate/support": "^10"
    },
    "autoload": {
        "psr-4": {
            "RenokiCo\\LaravelHealthchecks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenokiCo\\LaravelHealthchecks\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.3",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^8",
        "orchestra/testbench-core": "^8",
        "phpunit/phpunit": "^10"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "RenokiCo\\LaravelHealthchecks\\LaravelHealthcheckServiceProvider"
            ]
        }
    }
}
